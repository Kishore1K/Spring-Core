<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.jsp.Student" name="student1">
        <property name="studentId" value="12"/>
        <property name="name" value="Kishore"/>
        <property name="studentAddress" value="KGF"/>
    </bean>
<!--    2 Bean using p:schema-->
    <bean class="com.jsp.Student" name="student2" p:studentId="124343" p:name="Karthick" p:studentAddress="Bengaluru"  scope="prototype"/>

</beans>