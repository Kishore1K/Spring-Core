<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.spring.auto.wire.Address" name="address" p:city="KGF" p:state="KA" />
    <bean class="com.spring.auto.wire.Emp" name="emp" p:name="KISHORE" autowire="byName" />
    <bean class="com.spring.auto.wire.Emp" name="emp1" p:name="K" autowire="byType" />
    <bean class="com.spring.auto.wire.Emp" name="emp2" p:name="Kishore" autowire="constructor" />



</beans>